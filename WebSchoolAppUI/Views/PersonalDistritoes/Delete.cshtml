@model WebSchoolAppUI.Models.PersonalDistrito

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<h3>Estas seguro de eliminar este registro?</h3>
<div>
    <h4>Personal del Distrito</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.Apellido)
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(model => model.Cedula)
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.Cedula)
        </dd>
        <dt class="col-sm-4">
            Departamento
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.IdDepartamentoNavigation.Nombre)
        </dd>
        <dt class="col-sm-4">
            Distrito
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.IdDistritoNavigation.Codigo)
        </dd>
        <dt class="col-sm-4">
            Fecha de Creacion
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.FechaCreado)
        </dd>
        <dt class="col-sm-4">
            Fecha de Modificacion
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(model => model.FechaModificado)
        </dd>
      
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdPersonalDistrito" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar al Indice</a>
    </form>
</div>
