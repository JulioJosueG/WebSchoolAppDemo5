@model WebSchoolAppUI.Models.Usuario

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalle de usuario</h1>

<div>
    <h4>Usuario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Personal)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Personal)
        </dd>
       
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaCreado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCreado)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaModificado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaModificado)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdUsuario">Editar</a> |
    <a asp-action="Index">Regresar al Indice</a>
